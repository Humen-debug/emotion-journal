<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

    <Color x:Key="Primary">#AEB7C3</Color>
    <Color x:Key="PrimaryDark">#B0B7C0</Color>
    <Color x:Key="PrimaryDarkText">#242424</Color>
    <Color x:Key="Secondary">#CF9A88</Color>
    <Color x:Key="SecondaryDark">#C3846E</Color>
    <Color x:Key="Tertiary">#DCD2CC</Color>
    <Color x:Key="TertiaryDark">#595E60</Color>
    <Color x:Key="Background">#EDEAE3</Color>
    <Color x:Key="BackgroundDark">#171710</Color>

    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="Magenta">#D600AA</Color>
    <Color x:Key="MidnightBlue">#190649</Color>
    <Color x:Key="OffBlack">#1f1f1f</Color>
    <Color x:Key="DeepKoamaru">#2B2F6B</Color>
    <Color x:Key="CyberGrape">#554677</Color>
    <Color x:Key="PetiteOrchild">#DD978F</Color>
    <Color x:Key="Puce">#C17F8C</Color>
    <Color x:Key="Stikemaster">#9E6D82</Color>

    <Color x:Key="Gray100">#1C1C1C</Color>
    <Color x:Key="Gray200">#6D6D6D</Color>
    <Color x:Key="Gray300">#A5AAB0</Color>
    <Color x:Key="Gray400">#EEEEEE</Color>
    <Color x:Key="GrayDark100">#FFFFFD</Color>
    <Color x:Key="GrayDark200">#D9D9D9</Color>
    <Color x:Key="GrayDark300">#736E6E</Color>
    <Color x:Key="GrayDark400">#363535</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>

    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Gray100DarkBrush" Color="{StaticResource GrayDark100}"/>
    <SolidColorBrush x:Key="Gray200DarkBrush" Color="{StaticResource GrayDark200}"/>
    <SolidColorBrush x:Key="Gray300DarkBrush" Color="{StaticResource GrayDark300}"/>
    <SolidColorBrush x:Key="Gray400DarkBrush" Color="{StaticResource GrayDark400}"/>
</ResourceDictionary>